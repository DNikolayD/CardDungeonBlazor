@using CardDungeonBlazor.Areas.Cards.Models
@using Services.ServiceModels.CardsModels

<MudCard Class="player1-card-position" Style="@modelcss">
    <MudCardMedia Image="@Model.ImageUrl" Class="player1-card-position" ></MudCardMedia>
</MudCard>

@code {
    [Parameter]
    public CardViewModel Model { get; set; }
    [Parameter]
    public int z { get; set; }

    string modelcss => $"padding-top:{Model.Offcet}px; z-index:{z}";
}
